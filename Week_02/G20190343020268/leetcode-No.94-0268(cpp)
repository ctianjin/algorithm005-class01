//leetcode-No.94 
class Solution 
{ 
    public: 
        vector<int> inorderTraversal(TreeNode* root) 
        { 
            vector<int> ret; 
            rec(root,ret);
            return ret; 
        } 
        
    private:
        void rec(TreeNode* root,vector<int> &ret)
        { 
            if(root != NULL){ 
            rec(root->left,ret); 
            ret.push_back(root->val); 
            rec(root->right,ret); 
            } 
        }
};
